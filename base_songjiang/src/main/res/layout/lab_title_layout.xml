<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingTop="@dimen/unit_10"
    android:layout_below="@+id/rl_content"
    android:background="@color/white"
    >

    <View
        android:id="@+id/v_view1"
        android:layout_width="@dimen/unit_2.5"
        android:layout_height="@dimen/unit_12"
        android:layout_centerVertical="true"
        android:layout_marginLeft="@dimen/unit_10"
        android:layout_marginRight="@dimen/unit_10"
        android:background="@color/color_view_bg" />

    <TextView
        android:id="@+id/tv_labname"
        style="@style/text_style_07"
        android:layout_toRightOf="@+id/v_view1"
        tools:text="ddddd" />


    <include
        layout="@layout/line_layout"
        android:layout_below="@+id/tv_labname"
        android:layout_marginTop="@dimen/unit_10"
        android:layout_width="match_parent"
        android:layout_height="@dimen/unit_01" />


</RelativeLayout>