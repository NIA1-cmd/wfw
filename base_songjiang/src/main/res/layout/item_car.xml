<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/linear"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="@dimen/unit_10"
    android:layout_marginRight="@dimen/unit_02"
    android:orientation="vertical"
    android:paddingLeft="@dimen/unit_10"
    android:paddingRight="@dimen/unit_16"
    android:paddingTop="@dimen/unit_10"
    >

    <TextView
        android:id="@+id/tv_title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_marginBottom="@dimen/unit_10"
        android:layout_marginTop="@dimen/unit_10"
        android:text="车辆信息"
        android:textColor="@color/maincolor_01"
        android:textSize="@dimen/text_size_14"
        />

    <View
        android:layout_width="match_parent"
        android:layout_height="0.5dp"
        android:layout_marginBottom="@dimen/unit_10"
        android:layout_marginLeft="@dimen/unit_10"
        android:layout_marginRight="@dimen/unit_10"
        android:background="@color/orthercolor_03"
        />

    <com.oncloudsoft.sdk.view.BaseItemTextView
        android:id="@+id/tv_cartnumber"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:endIconVisibility="gone"
        app:labName="车牌号"
        />

    <com.oncloudsoft.sdk.view.BaseItemTextView
        android:id="@+id/tv_cllx"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:endIconVisibility="gone"
        app:labName="车辆类型"
        />

    <com.oncloudsoft.sdk.view.BaseItemTextView
        android:id="@+id/tv_clpp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:endIconVisibility="gone"
        app:labName="车辆品牌"
        />

    <com.oncloudsoft.sdk.view.BaseItemTextView
        android:id="@+id/tv_clsbh"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:endIconVisibility="gone"
        app:labName="车辆识别号"
        />

    <com.oncloudsoft.sdk.view.BaseItemTextView
        android:id="@+id/tv_zcrq"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:endIconVisibility="gone"
        app:labName="注册日期"
        />

    <com.oncloudsoft.sdk.view.BaseItemTextView
        android:id="@+id/tv_zcjg"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/unit_15"
        app:endIconVisibility="gone"
        app:labName="注册机构"
        />

</LinearLayout>
