<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">


    <TextView
        android:id="@+id/tv_execute_unit"
        style="@style/text_style_07"
        android:layout_marginLeft="@dimen/unit_20"

        android:layout_marginTop="@dimen/unit_10"
        tools:text="dfsfsdfsf" />

    <TextView
        android:id="@+id/tv_feedback_result"
        style="@style/text_style_07"
        android:layout_alignBaseline="@+id/tv_execute_unit"
        android:layout_alignParentRight="true"
        android:layout_marginRight="@dimen/unit_20"
        tools:text="dfsfsdfsf" />


    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_execute_unit"
        android:layout_marginTop="@dimen/unit_10"
        android:orientation="horizontal">

        <View
            android:layout_width="@dimen/unit_20"
            android:layout_height="0dp" />

        <include
            layout="@layout/line_layout"
            android:layout_width="match_parent"
            android:layout_height="@dimen/unit_01" />
    </LinearLayout>

</RelativeLayout>